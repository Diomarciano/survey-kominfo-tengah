<script>
function populate(form_barat_provinsi,form_barat_kabupaten){
  var form_barat_provinsi = document.getElementById(form_barat_provinsi);
  var form_barat_kabupaten = document.getElementById(form_barat_kabupaten);
  form_barat_kabupaten.innerHTML = "";
  if(form_barat_provinsi.value == "Aceh"){
    var optionArray = ["|","Aceh Besar|Aceh Besar","Aceh Singkil|Aceh Singkil"];
  } else if(form_barat_provinsi.value == "Sumatera Utara"){
    var optionArray = ["|","Nias|Nias","Nias Selatan|Nias Selatan"];
  } else if(form_barat_provinsi.value == "Riau"){
    var optionArray = ["|","Kepulauan Meranti|Kepulauan Meranti"];
  } else if(form_barat_provinsi.value == "Kepulauan Riau"){
    var optionArray = ["|","Natuna|Natuna","Bintan|Bintan"];
  } else if(form_barat_provinsi.value == "Bengkulu"){
    var optionArray = ["|","Bengkulu Utara|Bengkulu Utara","Seluma|Seluma"];
  } else if(form_barat_provinsi.value == "Sumatera Barat"){
    var optionArray = ["|","Pasaman Barat|Pasaman Barat","Solok Selatan|Solok Selatan"];
  } else if(form_barat_provinsi.value == "Sumatera Selatan"){
    var optionArray = ["|","Musi Rawas|Musi Rawas","Musi Rawas Utara|Musi Rawas Utara"];
  } else if(form_barat_provinsi.value == "Lampung"){
    var optionArray = ["|","Pesisir Barat|Pesisir Barat"];
  } else if(form_barat_provinsi.value == "Banten"){
    var optionArray = ["|","Lebak|Lebak","Pandeglang|Pandeglang"];
  } else if(form_barat_provinsi.value == "Jawa Timur"){
    var optionArray = ["|","Bondowoso|Bondowoso"];
  }
  for(var option in optionArray){
    var pair = optionArray[option].split("|");
    var newOption = document.createElement("option");
    newOption.value = pair[0];
    newOption.innerHTML = pair[1];
    form_barat_kabupaten.options.add(newOption);
  }
}
</script>