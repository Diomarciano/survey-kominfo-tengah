<div class="container">
  <div class="table-responsive">
    <div class="col-md-12">          
      <table class="table table-bordered table-striped table-hover table-mc-light-blue">
        <thead>
          <tr>
            <th style="text-align: center;">Provinsi</th>
            <th style="text-align: center;">Kota / Kabupaten</th>
            <th style="text-align: center;">Kecamatan</th>
            <th style="text-align: center;">Kelurahan</th>
            <th style="text-align: center;">Lattide</th>
            <th style="text-align: center;">Longitude</th>
          </tr>
        </thead>

        <tbody>
          <% @form_barats.each do |form_barat| %>
            <tr>
              <td data-title="Provinsi"><%= form_barat.provinsi %></td>
              <td data-title="Kota/Kabupaten"><%= form_barat.kabupaten %></td>
              <td data-title="Kota/Kabupaten"><%= form_barat.kecamatan %></td>
              <td data-title="Kota/Kabupaten"><%= form_barat.kelurahan %></td>
              <td data-title="Kecamatan"><%= form_barat.lattide %></td>
              <td data-title="Kelurahan"><%= form_barat.longitude %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>       
