              <div class="panel-body">
                <div class= "col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <%= pie_chart FormBarat.where(provinsi:"Aceh").group(:kabupaten).count, discrete: true, donut:true, height: "400px", messages: {empty: "No data"} %>
                </div>
              </div>
            </div>
        </div>
  </div>     

